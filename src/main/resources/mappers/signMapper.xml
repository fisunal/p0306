<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.mappers.signMapper">
	<insert id="signUp" parameterType="com.company.VO.SignUpVO">insert into member_info values(#{id},#{password},#{name},#{hp},#{email},#{address})</insert>
	<select id="signIn" resultType="RSIGNIN">select id,password from member_info</select>
</mapper>